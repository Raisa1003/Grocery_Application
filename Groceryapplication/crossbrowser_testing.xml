<suite name="crosbrowser_testing" parallel="tests" thread-count="2">
	<test thread-count="5" name="chromeTest">
	<parameter name="browser" value="Chrome"></parameter>
		<classes>

			<class name="testscripts.LoginTest">
				<methods>
					<include
						name="verifyUserLoginWithInvalidUsernameAndValidPassword"></include>
				</methods>
			</class>
		</classes>
	</test>
	<test thread-count="5" name="edge-test">
	<parameter name="browser" value="Edge"></parameter>
		<classes>
	<class name="testscripts.SwapnaNewTest">
				<methods>
					<include
						name="verifyUserIsAbleToEditContactDetails"></include>
				</methods>
			</class>
		</classes>
	</test>
</suite>